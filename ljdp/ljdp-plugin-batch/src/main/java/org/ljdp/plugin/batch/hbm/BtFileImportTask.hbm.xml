<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ljdp.plugin.batch.persistent.BtFileImportTask" table="BATCH_FILEIMPORT_TASK">
<id column="TASK_ID" name="taskId" type="java.lang.String"/>

<property column="TASK_NAME" length="100" name="taskName"  type="java.lang.String"/>
<property column="TASK_TYPE" length="30" name="taskType"  type="java.lang.String"/>
<property column="OPER_TYPE" length="30" name="operType"  type="java.lang.String"/>
<property column="OPERATOR" length="20" name="operator"  type="java.lang.String"/>
<property column="OPERATOR_NAME" length="30" name="operatorName"  type="java.lang.String"/>
<property column="BS_BO" length="50" name="bsBo"  type="java.lang.String"/>
<property column="TOTAL_REC"  name="totalRec"  type="java.lang.Integer"/>
<property column="SUCC_REC"  name="succRec"  type="java.lang.Integer"/>
<property column="FAIL_REC" name="failRec"  type="java.lang.Integer"/>
<property column="FAIL_LOG" length="100" name="failLog"  type="java.lang.String"/>
<property column="PROC_STATE" length="15" name="procState"  type="java.lang.String"/>
<property column="PROC_WAY" length="10" name="procWay"  type="java.lang.String"/>
<property column="COST_TIME"  name="costTime"  type="java.lang.Long"/>
<property column="CREATE_DATE" name="createDate"  type="java.util.Date"/>
<property column="SERVER_IP" length="20" name="serverIP"  type="java.lang.String"/>
<property column="BEGIN_TIME" name="beginTime"  type="java.util.Date"/>
<property column="FINISH_TIME" name="finishTime"  type="java.util.Date"/>
<property column="PROCE_FILE_NAME" length="100" name="proceFileName"  type="java.lang.String"/>
</class>
</hibernate-mapping>
